{
  "name": "graph",
  "scopeName": "source.graph",
  "displayName": "Graph",
  "fileTypes": [
    "graph"
  ],
  "repository": {
    "$self": {},
    "$base": {}
  },
  "patterns": [
    {
      "name": "keyword.control.pestgraph",
      "match": "\\b(graph|down|up|left|right)\\b"
    },
    {
      "name": "entity.label.node",
      "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\b"
    },
    {
      "name": "keyword.operator.arrow",
      "match": "--|-\\.|==|~~"
    },
    {
      "name": "punctuation.definition.node",
      "match": "[\\(\\)\\{\\}\\[\\]]"
    },
    {
      "name": "keyword.operator.edge",
      "match": "<|>|\\||:"
    }
  ]
}
